{
    "version": 2,
    "builds": [
      {
        "src": "book_gen.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/*",
        "use": "@vercel/static"
      },
      {
        "src": "templates/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/book_gen.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/templates/(.*)",
        "dest": "/templates/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/templates/index.html"
      }
    ]
  }
  